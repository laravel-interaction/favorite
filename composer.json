{
    "name": "laravel-interaction/favorite",
    "description": "User favorite/unfavorite behaviour for Laravel.",
    "keywords": ["laravel", "favorites", "favorite"],
    "license": "MIT",
    "homepage": "https://github.com/laravel-interaction/favorite",
    "support": {
        "issues": "https://github.com/laravel-interaction/favorite/issues",
        "source": "https://github.com/laravel-interaction/favorite"
    },
    "authors": [
        {
            "name": "zingimmick",
            "email": "zingimmick@outlook.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.13 || ^9.0 || ^10.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0",
        "laravel-interaction/support": "^1.0 || ^2.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3.3 || ^1.4.2",
        "orchestra/testbench": "^6.0 || ^7.0 || ^8.0",
        "phpunit/phpunit": "^9.3.3 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelInteraction\\Favorite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelInteraction\\Favorite\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "LaravelInteraction\\Favorite\\FavoriteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
